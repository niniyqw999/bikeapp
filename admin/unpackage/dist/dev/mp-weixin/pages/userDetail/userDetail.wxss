
.header {
		width: 100%;
		height: 500rpx;
		background-color: #1296db;
		border-radius: 0 0 5% 5%;
		margin-bottom: 40rpx;
		box-shadow: 0 2rpx 10rpx 0;
		display: flex;
		justify-content: center;
		align-items: center;
}
.picture {
		height: 200rpx;
		width: 200rpx;
		border-radius: 50%;
		background-color: white;
}
