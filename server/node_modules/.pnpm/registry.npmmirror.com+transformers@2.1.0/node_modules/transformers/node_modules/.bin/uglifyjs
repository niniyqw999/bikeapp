#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+uglify-js@2.2.5/node_modules/uglify-js/bin/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+uglify-js@2.2.5/node_modules/uglify-js/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+uglify-js@2.2.5/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+uglify-js@2.2.5/node_modules/uglify-js/bin/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+uglify-js@2.2.5/node_modules/uglify-js/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+uglify-js@2.2.5/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+uglify-js@2.2.5/node_modules/uglify-js/bin/uglifyjs" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+uglify-js@2.2.5/node_modules/uglify-js/bin/uglifyjs" "$@"
fi
