#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+jade@1.11.0/node_modules/jade/bin/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+jade@1.11.0/node_modules/jade/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+jade@1.11.0/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+jade@1.11.0/node_modules/jade/bin/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+jade@1.11.0/node_modules/jade/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/registry.npmmirror.com+jade@1.11.0/node_modules:/mnt/d/wxapp/bikeApp/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jade/bin/jade.js" "$@"
else
  exec node  "$basedir/../jade/bin/jade.js" "$@"
fi
